<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.insectcatalog.mappers.InsectKindMapper">
  <select id="selectAll" resultType="com.example.demo.insectcatalog.beans.InsectKindType">
    SELECT 
      insect_kind.insect_id
      ,insect_kind.species_name 
    FROM insect_kind
    ORDER BY insect_id
  </select>
</mapper>